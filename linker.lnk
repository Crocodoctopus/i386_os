OUTPUT_FORMAT(binary)
OUTPUT_ARCH(i386)
SECTIONS {
	STAGE2_SRC = (LOADADDR(.stage2) + 511)/512 + 1;
	STAGE2_DST = ADDR(.stage2);
	STAGE2_SIZE = (.end - .start + 511)/512;
	
	. = 0x7C00;
	.stage1 : AT(0x0) {
		tmp/stage1.o (.stage1)
	}
	.bootsig : AT(0x1FE) {
		tmp/stage1.o (.bootsig)
	}

	. = 0x8000;
	.start = .;
	.stage2 : AT(0x200) {
		tmp/stage2.o (.stage2)
	}

	text : {
		*(.text)
	}
	.data : ALIGN(4) {
		*(.data)
	}
	.end = .;
}
